{
    "name": "vuethers",
    "version": "0.3.1",
    "private": false,
    "description": "A DApp frontend framework for Vue3 built with Ethers (ethers.js)",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/0Lilian/vuethers.git"
    },
    "bugs": "https://github.com/0Lilian/vuethers/issues",
    "files": [
        "dist"
    ],
    "main": "./dist/vuethers.umd.js",
    "module": "./dist/vuethers.es.js",
    "unpkg": "./dist/vuethers.min.js",
    "exports": {
        ".": {
            "import": "./dist/vuethers.es.js",
            "require": "./dist/vuethers.umd.js"
        }
    },
    "keywords": [
        "dapp",
        "frontend",
        "vue",
        "vue3",
        "vuejs",
        "vuejs",
        "web3",
        "ethersjs",
        "ethers",
        "unstyled-components",
        "ethereum"
    ],
    "scripts": {
        "dev": "npx vite",
        "build": "npx vite build"
    },
    "dependencies": {
        "vue": "^3.2.37",
        "ethers": "^5.6.9"
    },
    "devDependencies": {
        "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.13",
        "@vitejs/plugin-vue": "^3.0.1",
        "hardhat": "^2.10.1",
        "hardhat-deploy": "^0.11.12",
        "vite": "^3.0.4"
    }
}
